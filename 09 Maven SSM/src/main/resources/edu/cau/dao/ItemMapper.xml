<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.cau.dao.ItemMapper">
    <select id="findItem" parameterType="_int" resultType="Item">
         select * from item where id = #{id}
    </select>
</mapper>